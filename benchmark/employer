class Person {
    int salary;
    Person boss;
    Person employee;  
    

    inv: 
    this.salary = null & this.boss = null ;
    this.boss -> p * (p.employee -> this) @ L * (this.salary -> v) @ L * (p.salary -> w) @ L & v < w ;
    (this.employee -> e)@B(e.boss -> this) * (this.salary -> n) @ L * (e.salary -> m) @ L & m < n ; 

    int get_salary()

    req: this.salary -> v ens: this.salary -> v & res = v 
    req_inv: inv ens_inv: inv 

    {
        return salary;
    }

    Person get_boss()
        similar
    {
        return boss;
    }

      Person get_employee()
        similar
    {
        return employee;
    }
         

    void set_salary (int x)
        req (this.salary -> v) 


}